<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="net.geni.aggregate.services.core.AggregateUser" table="users">
      <id column="id" name="id"/>
      <property column="name" name="name"/>
      <property column="password" name="password"/>
      <property column="role" name="role"/>
      <property column="certSubject" name="certSubject"/>
      <property column="firstName" name="firstName"/>
      <property column="lastName" name="lastName"/>
      <property column="email" name="email"/>
      <property column="description" name="description"/>
  </class>
</hibernate-mapping>
